                    <th>LastName</th>
                    <th>Current Team</th>
                    <th>Player Id</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var j in Model.Players.Where(j => j.LastName == "Cooper" && j.FirstName != "Joshua")){
                    <tr>
                        <td scope="row">@j.FirstName</td>
                        <td>@j.LastName</td>
                        <td>@j.CurrentTeam.TeamName</td>
                        <td>@j.PlayerId</td>
                    </tr>
