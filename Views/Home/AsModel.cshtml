                <tr>
                    <th>Name</th>
                    <th>Sport</th>
                    <th>Created</th>
                    <th>Updated</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var a in Model.Leagues.Where(a => a.Name.Contains("Atlantic")).ToList()){
