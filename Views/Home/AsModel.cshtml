    <div class="col-lg-5">
        <h3>Alphebet</h3>
        <table class="table table-striped table-inverse table-responsive">
            <thead class="thead-inverse">
                <tr>
                    <th>Name</th>
                    <th>Location</th>
                    <th>League</th>
                    <th>LeagueId</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var a in Model.Teams.OrderBy(a => a.Location).ToList()){
                    <tr>
                        <td scope="row">@a.TeamName</td>
                        <td>@a.Location</td>
                        <td>@a.CurrLeague.Name</td>
                        <td>@a.LeagueId</td>
                    </tr>
                    }
                </tbody>
        </table>
    </div>
    <div class="col-lg-5">
        <h3>Reverse Alphebet</h3>
        <table class="table table-striped table-inverse table-responsive">
            <thead class="thead-inverse">
                <tr>
                    <th>Name</th>
                    <th>Location</th>
                    <th>League</th>
                    <th>LeagueId</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var a in Model.Teams.OrderByDescending(a => a.Location).ToList()){
                    <tr>
                        <td scope="row">@a.TeamName</td>
                        <td>@a.Location</td>
                        <td>@a.CurrLeague.Name</td>
                        <td>@a.LeagueId</td>
                    </tr>
                    }
                </tbody>
        </table>
    </div>
    <div class="col-lg-5">
        <h3>LastName Cooper</h3>
        <table class="table table-striped table-inverse table-responsive">
            <thead class="thead-inverse">
                <tr>
                    <th>FirstName</th>
                    <th>LastName</th>
                    <th>Current Team</th>
                    <th>Player Id</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var c in Model.Players.Where(c => c.LastName == ("Cooper")).ToList()){
                    <tr>
                        <td scope="row">@c.FirstName</td>
                        <td>@c.LastName</td>
                        <td>@c.CurrentTeam.TeamName</td>
                        <td>@c.PlayerId</td>
                    </tr>
                    }
                </tbody>
        </table>
    </div>
    <div class="col-lg-5">
        <h3>Joshin Around</h3>
        <table class="table table-striped table-inverse table-responsive">
