    <div class="col-lg-5">
        <h3>Dallas</h3>
        <table class="table table-striped table-inverse table-responsive">
            <thead class="thead-inverse">
                <tr>
                    <th>Name</th>
                    <th>Location</th>
                    <th>League</th>
                    <th>LeagueId</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var d in Model.Teams.Where(d => d.Location != "Dallas").ToList()){
                    <tr>
                        <td scope="row">@d.TeamName</td>
                        <td>@d.Location</td>
                        <td>@d.CurrLeague.Name</td>
